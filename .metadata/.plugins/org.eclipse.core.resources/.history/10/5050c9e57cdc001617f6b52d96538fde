#!/usr/bin/env python
#coding:UTF-8

class Car(object):

    material = 'metal'
    
    def __init__(self, brand, colour, engin):
        self.Brand = brand
        self.Colour = colour
        self.__Engin = engin

    def move(self, direction):
        print(direction)
    
    @staticmethod
    def bar():
        print('staticmethod1')

    @property
    def Foo(self):
        return('something')

    def read(self):
        print(self.__Engin)
    

car1 = Car('BMW', 'Red', 'AAA')
#print(car1.Brand)
#print(car1.Colour)
#print(Car.material)
#car1.move('backward')
#Car.bar()
#result = car1.Foo
#print(result)
car1.read()











