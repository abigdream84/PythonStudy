#!/usr/bin/env python
#coding:UTF-8

import socket

ip_addr = ('127.0.0.1',9999)

sk = socket.socket()
sk.connect(ip_addr)

while True:
    data = sk.recv(1024)
    print(data)
    res = input('client:')
    print(res)
    print(type(res))
    sk.send(res)
    if res == 'exit':
        break
sk.close()




