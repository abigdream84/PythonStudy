#!/usr/bin/env python
#coding:UTF-8

import threading
import time

num = 0
def run():
    time.sleep(1)
    global num
    lock.acquire()
    num += 1
    lock.release()
    time.sleep(1)
    print(num)


lock = threading.Lock()
for i in range(100):
    t = threading.Thread(target=run)
    t.start()




