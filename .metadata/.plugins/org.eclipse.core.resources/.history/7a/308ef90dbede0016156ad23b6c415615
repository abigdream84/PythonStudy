import pymysql

sql = "delete from %s where id=%s"
params = ('test',6)

db = pymysql.connect(host="127.0.0.1",user="root",password="root",database="ebankdb")
cursor = db.cursor()
reCount = cursor.execute(sql,params)
db.commit()
#data = cursor.fetchall()
print(reCount)
#print(data)
cursor.close()
db.close()
















