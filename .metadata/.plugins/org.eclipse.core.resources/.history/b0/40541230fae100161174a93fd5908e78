#!/usr/bin/env python
#coding:UTF-8

import socket

ip_addr = ('127.0.0.1',9999)

client = socket.socket()
client.connect(ip_addr)

while True:
    data = client.recv(1024)
    print(data)
#    res=input('client:')
#    print(res)
#    print(type(res))
#    client.send(res)
#    if res == 'exit':
#        break
  
client.close()




