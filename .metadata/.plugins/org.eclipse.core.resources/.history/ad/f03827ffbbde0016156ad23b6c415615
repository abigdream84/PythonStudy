import pymysql

db = pymysql.connect(host="127.0.0.1",user="root",password="root",database="ebankdb")
cursor = db.cursor()
reCount = cursor.execute("select * from test")
data = cursor.fetchone()
print(reCount)
print(data)
cursor.scroll(3,mode='relative')
data = cursor.fetchone()
print(reCount)
print(data)
cursor.close()
db.close()

















